{
  "name": "linter-gcc",
  "main": "./lib/main",
  "version": "0.5.9",
  "description": "Lint C and C++ source files using gcc / g++",
  "keywords": [
    "lint",
    "linter",
    "c",
    "c++",
    "gcc",
    "g++",
    "error"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/hebaishi/linter-gcc"
  },
  "license": "MIT",
  "engines": {
    "atom": ">0.50.0"
  },
  "providedServices": {
    "linter": {
      "versions": {
        "1.0.0": "provideLinter"
      }
    }
  },
  "dependencies": {
    "atom-linter": "^2.0.5",
    "atom-package-deps": "^2.0.1"
  },
  "package-deps": [
    "linter"
  ],
  "devDependencies": {
    "eslint": "latest"
  },
  "eslintConfig": {
    "env": {
      "es6": true,
      "browser": true,
      "node": true
    },
    "ecmaFeatures": {
      "modules": true
    },
    "globals": {
      "atom": true
    }
  }
}
